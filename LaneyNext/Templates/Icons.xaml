    </DataTemplate>
    <DataTemplate x:Key="Icon8Play">
        <Viewbox Stretch="Uniform">
            <PathIcon HorizontalAlignment="Center" VerticalAlignment="Center" Width="8" Height="8" Data="M 0,0 L 8,4 L 0,8 z"/>
        </Viewbox>
    </DataTemplate>
    <DataTemplate x:Key="Icon8Pause">
