    <!-- VKPay label -->
    <DataTemplate x:Key="VKPayLabel">
        <StackPanel Orientation="Horizontal" Height="20" Margin="8,7,8,7">
            <TextBlock VerticalAlignment="Center" FontSize="17" Margin="0,-3,6,0" Text="{Binding Converter={StaticResource loc}, ConverterParameter=botbtn_paywith}"/>
            <ContentPresenter Height="20" ContentTemplate="{StaticResource VKPayIcon}"/>
        </StackPanel>
    </DataTemplate>

