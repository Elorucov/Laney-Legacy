    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <DataTemplate x:Key="ImportantMessageIconTemplate">
        <FontIcon Foreground="{ThemeResource VKTextSubheadBrush}" FontSize="12" FontFamily="Segoe MDL2 Assets" Glyph=""/>
    </DataTemplate>

    <DataTemplate x:Key="UnreadMessageIconTemplate">
        <FontIcon Foreground="{ThemeResource VKTextSubheadBrush}" FontSize="12" FontFamily="Segoe MDL2 Assets" Glyph=""/>
    </DataTemplate>

    <DataTemplate x:Key="SendingMessageIconTemplate">
        <FontIcon Foreground="{ThemeResource VKTextSubheadBrush}" FontSize="12" FontFamily="Segoe MDL2 Assets" Glyph=""/>
    </DataTemplate>

    <DataTemplate x:Key="DeletedMessageIconTemplate">
        <FontIcon Foreground="{ThemeResource VKTextSubheadBrush}" FontSize="12" FontFamily="Segoe MDL2 Assets" Glyph=""/>
    </DataTemplate>

    <DataTemplate x:Key="FailedMessageIconTemplate">
        <FontIcon Foreground="Red" FontSize="12" FontFamily="Segoe MDL2 Assets" Glyph=""/>
    </DataTemplate>

    <DataTemplate x:Key="OnlineIcon">
        <Ellipse Width="12" Height="12" Fill="#8AC176" Stroke="{ThemeResource VKBackgroundContentBrush}" StrokeThickness="2"/>
    </DataTemplate>

